{% extends "base.html.twig" %}

{% block title %}
	Création d'une catégorie
{% endblock %}

{% block body %}
	<h1>Nouvelle Catégorie</h1>

	{{ form_start(formView) }}

	{{ form_errors(formView) }}

	{{ form_widget(formView) }}

	<button type="submit" class="btn btn-primary">
		<i class="fas fa-save">
			Sauvegarder</i>
	</button>

	{{ form_end(formView) }}
{% endblock %}
